{
  "name": "gadekar-arts",
  "version": "0.1.0",
  "private": true,
  "homepage": "/gadekar-arts/home",
  "dependencies": {
    "@material-ui/core": "^5.0.0-alpha.19",
    "@material-ui/icons": "^4.11.2",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.2",
    "@testing-library/user-event": "^12.5.0",
    "autoprefixer": "^9.8.6",
    "gh-pages": "^2.0.1",
    "postcss": "^8.2.1",
